
public class RandomArray {

	public static void main(String[] args) {
		int[] myList = new int[7];

		for (int i = 0; i < myList.length; i++) {
			  myList[i] = 1 + (int) (Math.random() * 65);
			  System.out.println(myList[i] + " ");
			  int total = 0;
			  total += myList[i];
			  System.out.println(total);
			}


	}

}
